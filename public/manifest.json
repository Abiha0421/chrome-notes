{
  "manifest_version": 3,
  "name": "I Notes",
  "version": "1.1",
  "description": "A simple extension to take notes / developer commands / passwords / etc",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "assets/note.png",
    "128": "assets/note.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]

}
